{
    "manifest_version": 2,
    "name": "Borderify",
    "version": "1.0",
    "description": "Adds a red border to all webpages matching mozilla.org.",
    "icons": 
    {
        "48": "icons/evoli.png"
    },
    "content_scripts": 
    [
      {
        "matches": ["<all_urls>"],
        "js": ["myfirstextension.js"]
      }
    ]
}